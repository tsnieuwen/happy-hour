  },
  "homepage": "https://github.com/tsnieuwen/happy-hour#readme",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-openapi-validator": "^5.0.2",
    "jest": "^29.4.3",
    "knex": "^2.4.2",
    "objection": "^3.0.1",
    "pg": "^8.9.0",
    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
